{
  "name": "Article",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "author": {
      "type": "belongsTo",
      "model": "Author",
      "foreignKey": "author_id"
    },
    "comments": {
      "type": "hasMany",
      "model": "Comment",
      "foreignKey": ""
    },
    "tags": {
      "type": "hasMany",
      "model": "Tag",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
